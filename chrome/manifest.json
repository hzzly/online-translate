{
  "manifest_version": 2,

  "name": "Online Translate",
  "description": "online translation",
  "version": "1.0.0",
  "icons": { "128": "icon_128.png" },

  "browser_action": {
    "default_icon": "icon_19.png",
    "default_popup": "index.html",
    "default_title": "Online Translate"
  },

  "permissions": [
    "http://openapi.youdao.com/api"
  ]
}